public class EvenNumbers {
    public static void main(String[] args) {
        // Pętla przechodząca przez liczby od 1 do 100
        for (int i = 1; i <= 100; i++) {
            // Sprawdzenie, czy liczba jest parzysta
            if (i % 2 == 0) {
                // Wypisanie liczby parzystej
                System.out.println(i);
            }
        }
    }
}
